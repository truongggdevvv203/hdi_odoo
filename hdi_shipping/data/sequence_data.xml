<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <!-- Sequence for Shipping Order Numbers -->
  <record id="seq_shipping_order" model="ir.sequence">
    <field name="name">Shipping Order Number</field>
    <field name="code">shipping.order</field>
    <field name="prefix">SO</field>
    <field name="padding">5</field>
    <field name="number_next">1</field>
  </record>
</odoo>
