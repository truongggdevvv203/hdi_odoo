<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence cho Shipping Order -->
        <record id="seq_shipping_order" model="ir.sequence">
            <field name="name">Shipping Order Sequence</field>
            <field name="code">shipping.order</field>
            <field name="prefix">SO/%(year)s/%(month)s/</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="padding">5</field>
        </record>
    </data>
</odoo>
